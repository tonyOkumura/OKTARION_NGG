# Redis Configuration for OKTARION NGG Cache
# ===========================================

# Basic Redis Settings
REDIS_PASSWORD=cachepass
REDIS_PORT=6379
REDIS_BIND=0.0.0.0

# Memory Management
REDIS_MAX_MEMORY=256mb
REDIS_MAX_MEMORY_POLICY=allkeys-lru

# Persistence Settings
REDIS_SAVE_900_1=900 1
REDIS_SAVE_300_10=300 10
REDIS_SAVE_60_10000=60 10000
REDIS_APPEND_ONLY=yes
REDIS_APPEND_FSYNC=everysec

# Logging
REDIS_LOG_LEVEL=notice

# Performance Settings
REDIS_TCP_KEEPALIVE=300
REDIS_TIMEOUT=0

# Cache Settings
CACHE_DEFAULT_TTL=3600
CACHE_MAX_KEYS=10000
CACHE_SESSION_TTL=86400

# Environment
ENVIRONMENT=development
SYSTEM_VERSION=1.0.0

# Security
REDIS_PROTECTED_MODE=yes
